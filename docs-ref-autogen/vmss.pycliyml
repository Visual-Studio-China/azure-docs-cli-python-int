commandList:
- name: az vmss create
  description: Create an Azure Virtual Machine Scale Set.
  href: '#create'
- name: az vmss deallocate
  description: Deallocate virtual machines in a scale set.
  href: '#deallocate'
- name: az vmss delete
  description: Deletes a VM scale set.
  href: '#delete'
- name: az vmss delete-instances
  description: Delete virtual machines in a scale set.
  href: '#delete-instances'
- name: az vmss diagnostics
  description: Configure the Azure VMSS diagnostics extension.
  href: diagnostics
  isGroup: true
- name: az vmss extension
  description: Extend the functionality of your scale-set with VM extensions.
  href: extension
  isGroup: true
- name: az vmss get-instance-view
  description: Get instance view for a scale set or its VM instances.
  href: '#get-instance-view'
- name: az vmss list
  description: List scale sets.
  href: '#list'
- name: az vmss list-instances
  description: Gets a list of all virtual machines in a VM scale sets.
  href: '#list-instances'
- name: az vmss list-skus
  description: Gets a list of SKUs available for your VM scale set, including the minimum and maximum VM instances allowed for each SKU.
  href: '#list-skus'
- name: az vmss nic
  description: ''
  href: nic
  isGroup: true
- name: az vmss reimage
  description: Reimage virtual machines in a virtual machine scale set.
  href: '#reimage'
- name: az vmss restart
  description: Restart virtual machines in a scale set.
  href: '#restart'
- name: az vmss scale
  description: Change the number of VMs in an virtual machine scale set.
  href: '#scale'
- name: az vmss show
  description: Show scale set or its VM instance.
  href: '#show'
- name: az vmss start
  description: Start virtual machines in a virtual machine scale set.
  href: '#start'
- name: az vmss stop
  description: Power off (stop) virtual machines in a virtual machine scale set.
  href: '#stop'
- name: az vmss update
  description: ''
  href: '#update'
- name: az vmss update-instances
  description: Upgrade virtual machines in a virtual machine scale set.
  href: '#update-instances'
htmlId: vmss
examples: []
source:
  remote:
    path: src/command_modules/azure-cli-vm/azure/cli/command_modules/vm/_help.py
    branch: master
    repo: https://github.com/Azure/azure-cli.git
children:
- commandList: []
  htmlId: create
  examples:
  - title: Windows scaleset with 5 instances, a load balancer and a public IP address
    code: az vmss create -n myName -g myResourceGroup --admin-password MyPassword123 --instance-count 5 --image Win2012R2Datacenter
  - title: Linux scaleset with SSH authentication, a public IP address, a DNS entry, an existing load balancer, and an existing virtual network
    code: az vmss create  -n myName -g myResourceGroup --dns-name-for-public-ip myGloballyUniqueDnsName --load-balancer myLoadBalancer --vnet-name myVNET --subnet mySubnet --image <linux image from 'az vm image list'> --ssh-key-value "<ssh-key-value or ssh-key-file-path>"
  parameters:
  - name: --image
    isRequired: True
    summary: OS image (URN alias, URN or URI).
    description: >-
      URN aliases: CentOS, CoreOS, Debian, openSUSE, RHEL, SLES, UbuntuLTS, Win2008R2SP1, Win2012Datacenter, Win2012R2Datacenter.
                                          Example URN: MicrosoftWindowsServer:WindowsServer:2012-R2-Datacenter:latest
                                          Example URI: http://<storageAccount>.blob.core.windows.net/vhds/osdiskimage.vhd.
  - name: --name -n
    isRequired: True
    summary: Name of the virtual machine scale set.
    description: ''
  - name: --resource-group -g
    isRequired: True
    summary: Name of resource group.
    description: ''
  - name: --disable-overprovision
    isRequired: False
    summary: Overprovision option (see <a href="https://azure.microsoft.com/en-us/documentation/articles/virtual-machine-scale-sets-overview/">https://azure.microsoft.com/en-us/documentation/articles/virtual-machine-scale-sets-overview/</a> for details).
    description: ''
  - name: --generate-ssh-keys
    isRequired: False
    summary: Generate SSH public and private key files if missing.
    description: ''
  - name: --instance-count
    isRequired: False
    summary: Number of VMs in the scale set.
    description: ''
    defaultValue: 2
  - name: --location -l
    isRequired: False
    summary: Location in which to create VM and related resources. Defaults to the resource group's location.
    description: ''
  - name: --no-wait
    isRequired: False
    summary: Do not wait for the long running operation to finish.
    description: ''
  - name: --tags
    isRequired: False
    summary: Space separated tags in 'key[=value]' format. Use "" to clear existing tags.
    description: ''
  - name: --upgrade-policy-mode
    isRequired: False
    summary: ''
    description: ''
    defaultValue: manual
    parameterValueGroup: Automatic, Manual
  - name: --validate
    isRequired: False
    summary: Generate and validate the ARM template without creating any resources.
    description: ''
  - name: --vm-sku
    isRequired: False
    summary: Size of VMs in the scale set.  See <a href="https://azure.microsoft.com/en-us/pricing/details/virtual-machines/">https://azure.microsoft.com/en-us/pricing/details/virtual-machines/</a> for size info.
    description: ''
    defaultValue: Standard_D1_v2
  - name: --admin-password
    isRequired: False
    summary: Password for the VM if authentication type is 'Password'.
    description: ''
  - name: --admin-username
    isRequired: False
    summary: Username for the VM.
    description: ''
    defaultValue: appveyor
  - name: --authentication-type
    isRequired: False
    summary: Type of authentication to use with the VM. Defaults to password for Windows and SSH public key for Linux.
    description: ''
    parameterValueGroup: password, ssh
  - name: --ssh-dest-key-path
    isRequired: False
    summary: Destination file path on the VM for the SSH key.
    description: ''
  - name: --ssh-key-value
    isRequired: False
    summary: SSH public key or public key file path.
    description: ''
  - name: --backend-pool-name
    isRequired: False
    summary: Name to use for the backend pool when creating a new load balancer.
    description: ''
  - name: --backend-port
    isRequired: False
    summary: Backend port to open with NAT rules. Defaults to 22 on Linux and 3389 on Windows.
    description: ''
  - name: --load-balancer
    isRequired: False
    summary: Name to use when creating a new load balancer (default) or referencing an existing one. Can also reference an existing load balancer by ID or specify "" for none.
    description: ''
  - name: --nat-pool-name
    isRequired: False
    summary: Name to use for the NAT pool when creating a new load balancer.
    description: ''
  - name: --public-ip-address
    isRequired: False
    summary: Name of the public IP address when creating one (default) or referencing an existing one. Can also reference an existing public IP by ID or specify "" for None.
    description: ''
  - name: --public-ip-address-allocation
    isRequired: False
    summary: ''
    description: ''
    defaultValue: dynamic
    parameterValueGroup: dynamic, static
  - name: --public-ip-address-dns-name
    isRequired: False
    summary: Globally unique DNS name for a newly created Public IP.
    description: ''
  - name: --subnet
    isRequired: False
    summary: The name of the subnet when creating a new VNet or referencing an existing one. Can also reference an existing subnet by ID. If omitted, an appropriate VNet and subnet will be selected automatically, or a new one will be created.
    description: ''
  - name: --subnet-address-prefix
    isRequired: False
    summary: The subnet IP address prefix to use when creating a new VNet in CIDR format.
    description: ''
    defaultValue: 10.0.0.0/24
  - name: --vnet-address-prefix
    isRequired: False
    summary: The IP address prefix to use when creating a new VNet in CIDR format.
    description: ''
    defaultValue: 10.0.0.0/16
  - name: --vnet-name
    isRequired: False
    summary: Name of the virtual network when creating a new one or referencing an existing one.
    description: ''
  - name: --os-disk-name
    isRequired: False
    summary: The name of the new VM OS disk.
    description: ''
    defaultValue: osdiskimage
  - name: --os-type
    isRequired: False
    summary: Type of OS installed on a custom VHD. Do not use when specifiying an URN or URN alias.
    description: ''
    parameterValueGroup: linux, windows
  - name: --storage-caching
    isRequired: False
    summary: Storage caching type for the VM OS disk.
    description: ''
    defaultValue: ReadOnly
    parameterValueGroup: ReadOnly, ReadWrite
  - name: --storage-container-name
    isRequired: False
    summary: Name of the storage container for the VM OS disk.
    description: ''
    defaultValue: vhds
  - name: --storage-sku
    isRequired: False
    summary: The storage SKU to use for new storage accounts.
    description: ''
    defaultValue: Standard_LRS
    parameterValueGroup: Premium_LRS, Standard_GRS, Standard_LRS, Standard_RAGRS, Standard_ZRS
  - name: --debug
    isRequired: False
    summary: Increase logging verbosity to show all debug logs.
    description: ''
  - name: --help -h
    isRequired: False
    summary: Show this help message and exit.
    description: ''
  - name: --output -o
    isRequired: False
    summary: Output format.
    description: ''
    defaultValue: json
    parameterValueGroup: json, jsonc, list, table, tsv
  - name: --query
    isRequired: False
    summary: JMESPath query string. See <a href="http://jmespath.org/">http://jmespath.org/</a> for more information and examples.
    description: ''
  - name: --verbose
    isRequired: False
    summary: Increase logging verbosity. Use --debug for full debug logs.
    description: ''
  source:
    remote:
      path: src/command_modules/azure-cli-vm/azure/cli/command_modules/vm/_help.py
      branch: master
      repo: https://github.com/Azure/azure-cli.git
