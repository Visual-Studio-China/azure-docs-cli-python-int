commandList:
- name: az network nic ip-config address-pool
  description: Manage NIC IP configuration backend address pools.
  href: ip-config/address-pool
  isGroup: true
- name: az network nic ip-config address-pool add
  description: Add a backend address pool reference to an IP configuration.
  href: ip-config/address-pool#add
- name: az network nic ip-config address-pool remove
  description: Remove a backend address pool reference from an IP configuration.
  href: ip-config/address-pool#remove
- name: az network nic ip-config create
  description: Create a new IP configuration on a NIC.
  href: ip-config#create
- name: az network nic ip-config delete
  description: Delete an IP configuration from a NIC.
  href: ip-config#delete
- name: az network nic ip-config inbound-nat-rule
  description: Manage NIC IP configuration inbound NAT rules.
  href: ip-config/inbound-nat-rule
  isGroup: true
- name: az network nic ip-config inbound-nat-rule add
  description: Add an inbound NAT rule reference to an IP configuration.
  href: ip-config/inbound-nat-rule#add
- name: az network nic ip-config inbound-nat-rule remove
  description: Remove an inbound NAT rule reference from an IP configuration.
  href: ip-config/inbound-nat-rule#remove
- name: az network nic ip-config list
  description: List IP configurations on a NIC.
  href: ip-config#list
- name: az network nic ip-config show
  description: Show details of an IP configurations on a NIC.
  href: ip-config#show
- name: az network nic ip-config update
  description: Update an IP configurations on a NIC.
  href: ip-config#update
htmlId: ip-config
examples: []
