- commandList: []
  htmlId: login
  examples:
  - title: Log in interactively.
    code: az login
  - title: Log in with user name and password. This doesn't work with Microsoft accounts or accounts that have two-factor authentication enabled.
    code: az login -u johndoe@contoso.com -p VerySecret
  - title: Log in with a service principal.
    code: az login --service-principal -u http://azure-cli-2016-08-05-14-31-15 -p VerySecret --tenant contoso.onmicrosoft.com
  parameters:
  - name: --password -p
    isRequired: False
